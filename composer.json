{
    "name": "genert/laravel-invoice",
    "description": "Laravel package for downloading invoice with included default receipt template",
    "type": "library",
    "require": {
        "dompdf/dompdf": "^0.8.3",
        "illuminate/support": "^6.0",
        "symfony/http-kernel": "^4.3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Genert Org",
            "email": "contact@genert.org",
            "homepage": "http://genert.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Genert\\Laravel\\Invoice\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Genert\\Laravel\\Invoice\\InvoiceServiceProvider"
            ],
            "aliases": {
                "Invoice": "Genert\\Laravel\\Invoice\\Facades\\Invoice"
            }
        }
    }
}
